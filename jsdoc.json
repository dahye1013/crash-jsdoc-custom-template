{
  "source": {
    "include": ["src/"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "encoding": "utf8",
    "destination": "./docs/",
    "template": "custom-template",
    "tutorials": "./tutorials"
  }
}
